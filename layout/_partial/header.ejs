<header id="header">
  <div id="site-title">
    <a href="<%= config.url %>" id="logo">
      <img id="site-logo" src="<%- theme.root_url %>/img/blog-logo.png">
      <p><%= config.title %></p>
    </a>
  </div>
  <nav id="navbar">
    <ul id="menu">
      <% for (name in theme.menu) { %>
        <li class="menu-item">
          <a href="<%- url_for(theme.menu[name]) %>" class="menu-item-link">
            <%= name %>
          </a>
        </li>
      <% } %>
    </ul>
  </nav>
</header>